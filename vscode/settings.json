{
    "customizeUI.activityBar": "regular",
    "customizeUI.activityBarHideSettings": true,
    "customizeUI.stylesheet": {
        ".windows": "font-family: Fira Code Retina",
        ".sidebar.left .monaco-toolbar": "display: none",
    }, 
    "editor.bracketPairColorization.enabled": false,
    "editor.folding": false,
    "editor.fontFamily": "'Fira Code Retina', 'Fira Code Retina', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.hideCursorInOverviewRuler": true,
    "editor.guides.bracketPairs": false,
    "editor.guides.highlightActiveBracketPair": true,
    "editor.guides.highlightActiveIndentation": true,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": false,
    "editor.occurrencesHighlight": false,
    "editor.overviewRulerBorder": false,
    "editor.renderLineHighlight": "none",
    "editor.renderWhitespace": "none",
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "rules": {
            "keyword.controlFlow": {
                "fontStyle": "italic",
            },
            "*.mutable": {
                "fontStyle": "italic",
            },
            "keyword": {
                "fontStyle": "",
            },
            "operator": {
                "fontStyle": "",
            },
        }
    },
    "editor.tabSize": 4,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": true,
    "git.confirmSync": false,
    "gopls": {
        "ui.semanticTokens": true,
    },
    "remote.SSH.remotePlatform": {
        "parade": "linux",
        "paragon": "linux"
    },
    "security.workspace.trust.enabled": false,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.defaultProfile.windows": "",
    "terminal.integrated.fontFamily": "Fira Code Retina",
    "terminal.integrated.fontSize": 17,
    "vim.enableNeovim": true,
    "vim.leader": "<Space>",
    "vim.cursorStylePerMode.insert": "line-thin",
    "vim.cursorStylePerMode.normal": "block",
    "vim.highlightedyank.enable": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["K"],
            "commands": [
                "editor.action.showHover"
            ],
        },
        {
            "before": ["<C-k>"],
            "commands": [
                "editor.action.peekDefinition"
            ],
        },
    ],
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "",
    "vim.smartRelativeLine": true,
    "window.menuBarVisibility": "toggle",
    "window.restoreWindows": "none",
    "window.zoomLevel": -0.5,
    "workbench.colorCustomizations": {
        "[Kanagawa]": {
            "editorCursor.background": "#1F1F28",
            "editorGroupHeader.tabsBackground": "#1F1F28",
            "focusBorder": "#0000",
            "scrollbar.shadow": "#0000",
            "titleBar.activeBackground": "#1F1F28",
        },
    },
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Kanagawa",
    "workbench.editor.enablePreview": false,
    "workbench.editor.tabCloseButton": "off",
    "workbench.editor.untitled.hint": "hidden",
    "workbench.iconTheme": null,
    "workbench.layoutControl.enabled": false,
    "workbench.panel.defaultLocation": "right",
    "workbench.startupEditor": "none",
    "workbench.statusBar.visible": false,
    "workbench.tips.enabled": false,
    "zigLanguageClient.disabled": false,
    "zigLanguageClient.path": "",
}
