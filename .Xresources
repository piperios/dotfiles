XTerm*termName: xterm-256color
XTerm*locale: true
XTerm*faceName: IBM Plex Mono:size=11
XTerm*selectToClipboard: true
XTerm*cursorBlink: true
XTerm*ScrollBar: false

! Colors
*.background: #202020
*.foreground: #d0d0d0

! Black / Less black black :)
xterm*color0:  #202020
xterm*color8:  #606060

! Red / Bright red
xterm*color1:  #993d1f
xterm*color9:  #c25e5f

! Green / Bright green
xterm*color2:  #82994f
xterm*color10: #9bb26a

! Yellow / Bright yellow
xterm*color3:  #b3a657
xterm*color11: #f4bf77

! Blue / Bright blue
xterm*color4:  #5994ad
xterm*color12: #7baabd

! Magenta / Bright magenta
xterm*color5:  #a06594
xterm*color13: #b485aa

! Cyan / Bright cyan
xterm*color6:  #64aca0
xterm*color14: #86beb4

! Less white white :) / White
xterm*color7:  #d0d0d0
xterm*color15: #f5f5f5
